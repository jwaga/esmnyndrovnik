<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.eastsidemandala.nyndrovnik"
    android:versionCode="17"
    android:versionName="1.1.5">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="pl.eastsidemandala.nyndrovnik.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pl.eastsidemandala.nyndrovnik.AboutActivity"
            android:label="@string/title_activity_about"
            android:parentActivityName="pl.eastsidemandala.nyndrovnik.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pl.eastsidemandala.nyndrovnik.MainActivity" />
        </activity>
    </application>

</manifest>
